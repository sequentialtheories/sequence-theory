# Sequence Theory Environment Configuration
# Copy this file to .env and fill in the actual values

# API Configuration
VITE_TVC_API_BASE=https://your-tvc-api-domain.com/api
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Feature Flags
VITE_DISABLE_METAMASK=1
FEATURE_TESTNET_ONLY=1
SIMULATION_MODE=1

# Blockchain Configuration
CHAIN_ID=80002

# Sequence WaaS Configuration
VITE_SEQUENCE_PROJECT_ACCESS_KEY=your_sequence_project_access_key
VITE_SEQUENCE_WAAS_CONFIG_KEY=your_sequence_waas_config_key

# Vault Club API
VITE_VAULT_CLUB_API_KEY=your_vault_club_api_key

# Development Settings
VITE_APP_ENV=development
VITE_DEBUG_MODE=false

# Security Keys (Never commit actual values)
JWT_SECRET=your_jwt_secret_here
SERVICE_ROLE_KEY=your_service_role_key_here
SIGNING_SECRET=your_signing_secret_here
